select AVG(TO_NUMBER(SUBSTR(M.REQUEST_TIME-M.RECEIVE_TIME+M.SEND_TIME-M.RESPONSE_TIME,2,9))*24*3600000+
TO_NUMBER(SUBSTR(M.REQUEST_TIME-M.RECEIVE_TIME+M.SEND_TIME-M.RESPONSE_TIME,12,2))*3600000+
TO_NUMBER(SUBSTR(M.REQUEST_TIME-M.RECEIVE_TIME+M.SEND_TIME-M.RESPONSE_TIME,15,2))*60000+
TO_NUMBER(SUBSTR(M.REQUEST_TIME-M.RECEIVE_TIME+M.SEND_TIME-M.RESPONSE_TIME,18,2))*1000+
TO_NUMBER(SUBSTR(M.REQUEST_TIME-M.RECEIVE_TIME+M.SEND_TIME-M.RESPONSE_TIME,21,3))) from aip_message M
--csblishi,3.8241546702377 ms
select 
AVG(TO_NUMBER(SUBSTR(M.RESPONSE_TIME-M.REQUEST_TIME,2,9))*24*3600000+
TO_NUMBER(SUBSTR(M.RESPONSE_TIME-M.REQUEST_TIME,12,2))*3600000+
TO_NUMBER(SUBSTR(M.RESPONSE_TIME-M.REQUEST_TIME,15,2))*60000+
TO_NUMBER(SUBSTR(M.RESPONSE_TIME-M.REQUEST_TIME,18,2))*1000+
TO_NUMBER(SUBSTR(M.RESPONSE_TIME-M.REQUEST_TIME,21,3)))
from aip_message M
--busilishi ,1863.1828339471 ms
select 
AVG(TO_NUMBER(SUBSTR(M.SEND_TIME-M.RECEIVE_TIME,2,9))*24*3600000+
TO_NUMBER(SUBSTR(M.SEND_TIME-M.RECEIVE_TIME,12,2))*3600000+
TO_NUMBER(SUBSTR(M.SEND_TIME-M.RECEIVE_TIME,15,2))*60000+
TO_NUMBER(SUBSTR(M.SEND_TIME-M.RECEIVE_TIME,18,2))*1000+
TO_NUMBER(SUBSTR(M.SEND_TIME-M.RECEIVE_TIME,21,3)))
from aip_message M
--all lishi 1867.00698861734 ms
