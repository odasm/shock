SELECT
	     B.SERV_TYPE,A.SERV_STAT,C.BUSI_SERV_ADDR,
		         A.SERV_CODE,A.SERV_ID,  
		         A.SERV_NAME  
		   FROM SERV_AFFORD_MAN_REG A,SERV_DEFINE B,WSDL_CONF C 
		   WHERE A.SERV_ID=B.SERV_ID  
       AND B.WSDL_ID = C.WSDL_ID
		   AND A.SERV_ID=241;

SELECT B.SERV_TYPE,
       A.SERV_STAT,
       C.BUSI_SERV_ADDR,
       A.SERV_CODE,
       A.SERV_ID,
       A.SERV_NAME,
       B.VERSION_NO,
       B.VERSION_STATE
  FROM SERV_AFFORD_MAN_REG A, SERV_DEFINE B, WSDL_CONF C
 WHERE A.SERV_ID = B.SERV_ID
   AND B.WSDL_ID = C.WSDL_ID
   
   
